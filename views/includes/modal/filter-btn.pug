div.modal-footer.justify-content-md-center
  button.btn.btn-sm.material-button.material-secondary#dialogCancel(type='button', data-dismiss='modal')
    |  !{content.common.close}
  button.btn.btn-sm.material-button.material-primary.mx-1#dialogFilterReset(type='button' role="button")
    i.bi.bi-arrow-clockwise
    |  !{content.common.reset}
  button.btn.btn-sm.material-button.material-primary#dialogFilter(type='submit', role="button")
    i.bi.bi-funnel
    |  !{content.common.filter}